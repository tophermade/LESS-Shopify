<div id="page" class="{{page.handle}}">
	
	<div id="breadcrumb">
		<ul>
			<li><a href="/">Home &raquo;</a></li>
			<li><strong>{{ page.title }}</strong></li>
		</ul>
	</div>


	<header class="page-head">
		<h1>{{ page.title }}</h1>
	</header><!-- e: page header -->


	<section id="content">

		<article class="entry">
			{{ page.content }}
		</article>

	</section><!-- e: content -->


	{% include 'sidebar' %}


</div><!-- e: page -->